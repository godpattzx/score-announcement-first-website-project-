{
  "kind": "collectionType",
  "collectionName": "subjects",
  "info": {
    "singularName": "subject",
    "pluralName": "subjects",
    "displayName": "subject",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "staff": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "subject"
    },
    "CourseCode": {
      "type": "string"
    },
    "Lecturer": {
      "type": "string"
    },
    "views": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::view.view",
      "mappedBy": "subject"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published"
      ],
      "default": "draft"
    },
    "full_score": {
      "type": "integer",
      "default": 0
    },
    "publish_at": {
      "type": "datetime"
    }
  }
}
